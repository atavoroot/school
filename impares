package ex11;
import java.util.Scanner;

public class ex1 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int numeros;
		
			for(numeros = 1; numeros <=30; numeros++){
				if(numeros % 2 != 0){
					System.out.println(numeros);
				}
			}
	}

}
// números  ímpares  de  1  à  30 
